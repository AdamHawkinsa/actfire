{
  "name": "reactfire",
  "version": "0.0.1",
  "description": "Firebase library for React",
  "main": "index.js",
  "scripts": {
    "build": "tsc index.ts --lib DOM,ES2018 --sourceMap --declaration --jsx react",
    "test": "jest",
    "watch": "tsc index.ts --lib DOM,ES2018 --watch --sourceMap --declaration --jsx react",
    "docs": "./node_modules/.bin/typedoc --mode modules --json docs/reactfire-metadata.json index.ts && node ./docs/generate-readme.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FirebasePrivate/reactfire.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FirebasePrivate/reactfire/issues"
  },
  "homepage": "https://github.com/FirebasePrivate/reactfire#readme",
  "dependencies": {
    "firebase": "^5.9.4",
    "rxfire": "^3.3.11",
    "rxjs": "^6.4.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "react-hooks-testing-library": "^0.4.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  }
